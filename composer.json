{
    "name": "archlinux-de/flarum-click-image",
    "description": "Load images on click",
    "type": "flarum-extension",
    "license": "GPL-3.0-or-later",
    "require": {
        "flarum/core": "^1.0.0"
    },
    "extra": {
        "flarum-extension": {
            "title": "Click-Image",
            "icon": {
                "name": "fas fa-image",
                "backgroundColor": "#08c",
                "color": "#fff"
            }
        },
        "flarum-cli": {
            "modules": {
                "admin": false,
                "forum": true,
                "js": true,
                "jsCommon": false,
                "css": true,
                "locale": true,
                "gitConf": true,
                "githubActions": true,
                "prettier": true,
                "typescript": false,
                "bundlewatch": false,
                "backendTesting": false,
                "editorConfig": true,
                "styleci": true
            }
        }
    }
}
